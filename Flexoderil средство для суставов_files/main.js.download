$(document).ready(function() {

    $('body').append('<span href="#" id="popup_red_multi_lang_btn"></span>');

//  При нажатии на кнопку
    $('.toForm, *[href^="#form"], #popup_red_multi_lang_btn').on('click', function () {

        if ((document.location.hostname === 'd.zdorov-prostatite.com') || (document.location.hostname === 'b.zdorov-psoriaz.com') || (document.location.hostname === 'e.zdorov-gemoroy.com') || (document.location.hostname === 'h.zdorov-gemoroy.com') || (document.location.hostname === 'c.zdorov-psoriaz.com')) {
            $('.popup1').click();
            $('.rightZakaz').click();
            $('.zakazat').click();
            return false;
        }

        $('html, body').stop().animate({scrollTop: ($('.order-form:visible').last().offset().top - 50)}, 1000);
    });


    if ($("#zzzcpa").length) {
        $('.popup-zdorov-callback').on('click', function () {
            $('#zzzcpa').trigger('click');
        });
    } else {
        $('.popup-zdorov-callback').on('click', function () {
            $('html, body').stop().animate({scrollTop: ($('.order-form:visible').last().offset().top - 50)}, 1000);
        });
    };


    // VAT not incl.
    if (window.land_translates && window.land_translates.vat_not_incl) {
        var formwithprice = $('form.order-form');
        formwithprice.each(function () {
            if ($(this).closest('#zzzcpa-form').length == 0) {
                var tax_not_incl = '<div class="order-form__vat-not-incl">*' + window.land_translates.vat_not_incl + '</div>';

                $(this).find('button').after(tax_not_incl);
                $(this).find('input[type="submit"]').after(tax_not_incl);

                var curr = $('.order-form .price_land_curr, .order-form .price_land_curr2, .order-form .price_land_curr3');
                curr.each(function (){
                    if ($(this).parent().find('.price_land_s4').length == 0 && $(this).parent().text().indexOf('*') == -1)
                        $(this).after('<sup>*</sup>');
                });
            }
        })

    }
});


